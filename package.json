{
  "name": "google-chrome-bookmarks",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "build": "nx build",
    "watch": "nx build --watch --configuration development",
    "test": "nx test --watch=false && npx playwright test",
    "test:unit": "nx test --watch=false",
    "test:e2e": "npx playwright test",
    "postinstall": "npx playwright install",
    "affected:lint": "nx affected:lint",
    "start:dev": "nx run-many --target=build --all --configuration development --watch --parallel 5",
    "start:prod": "nx run-many --target=build --all --configuration production --watch --parallel 5",
    "build:dev": "nx run-many --target=build --all --configuration development --parallel 5",
    "build:prod": "nx run-many --target=build --all --configuration production --parallel 5"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.0.7",
    "@angular/cdk": "21.0.5",
    "@angular/common": "21.0.7",
    "@angular/compiler": "21.0.7",
    "@angular/core": "21.0.7",
    "@angular/forms": "21.0.7",
    "@angular/material": "21.0.5",
    "@angular/platform-browser": "21.0.7",
    "@angular/platform-browser-dynamic": "21.0.7",
    "@angular/router": "21.0.7",
    "@ngrx/operators": "21.0.1",
    "@ngrx/signals": "21.0.1",
    "@ngx-nova/material-extensions-table-virtual-scroll": "^2.0.2",
    "@types/chrome": "^0.0.246",
    "chrome-types": "^0.1.231",
    "ng2-dragula": "6.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.2",
    "@analogjs/vitest-angular": "^2.2.2",
    "@angular-devkit/build-angular": "21.0.5",
    "@angular-devkit/core": "21.0.5",
    "@angular-devkit/schematics": "21.0.5",
    "@angular/cli": "21.0.5",
    "@angular/compiler-cli": "21.0.7",
    "@nx/angular": "22.3.3",
    "@nx/vitest": "22.3.3",
    "@nx/workspace": "22.3.3",
    "@playwright/test": "^1.57.0",
    "@schematics/angular": "21.0.5",
    "jsdom": "^27.4.0",
    "nx": "22.3.3",
    "typescript": "5.9.3",
    "vite": "^7.0.0",
    "vitest": "^4.0.8"
  }
}
